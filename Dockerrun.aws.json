{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "basvanpol/fs-client",
            "hostname": "client",
            "essential": false,
            "links": ["api"],
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "containerPort": 8080
                }
            ]
        },
        {
            "name": "api",
            "image": "basvanpol/fs-api",
            "hostname": "api",
            "essential": false,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8090,
                    "containerPort": 8090
                }
            ]
        },
        {
            "name": "content-server",
            "image": "basvanpol/fs-content-server",
            "hostname": "content-server",
            "essential": false,
            "memory": 128
        },
        {
            "name": "nginx",
            "image": "basvanpol/fs-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "api"]
        }
    ]
}
