{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "basvanpol/fs-client",
            "hostname": "client",
            "essential": false,
            "links": ["server"],
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8080,
                    "containerPort": 8080
                }
            ]
        },
        {
            "name": "server",
            "image": "basvanpol/fs-server",
            "hostname": "server",
            "essential": false,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8090,
                    "containerPort": 8090
                }
            ]
        },
        {
            "name": "nginx",
            "image": "basvanpol/fs-nginx",
            "hostname": "nginx",
            "essential": true,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "server"]
        }
    ]
}
