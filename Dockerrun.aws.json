{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "name": "angular",
            "image": "basvanpol/fs-client",
            "hostname": "client",
            "essential": true,
            "links": ["server"],
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 8080
                }
            ]
        },
        {
            "name": "server",
            "image": "basvanpol/fs-server",
            "hostname": "server",
            "essential": false,
            "memory": 128,
            "portMappings": [
                {
                    "hostPort": 8090,
                    "containerPort": 8090
                }
            ]
        }
    ]
}
